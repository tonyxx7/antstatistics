    <dependency org="org.apache.ant" name="ant-launcher" rev="1.9.9"/>
    <dependency org="commons-lang" name="commons-lang" rev="2.6"/>
    <dependency org="com.thoughtworks.xstream" name="xstream" rev="1.3.1"/>
    <dependency org="org.jfree" name="jfreechart" rev="1.0.19"/>
    <dependency org="org.jfree" name="jcommon" rev="1.0.24"/>
  </dependencies>
</ivy-module>
